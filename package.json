{
    "name": "oko-ui",
    "version": "0.0.0",
    "homepage": "https://github.com/shbov/oko-ui",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shbov/oko-ui.git"
    },
    "license": "MIT",
    "sideEffects": [
        "*.scss",
        "*.css"
    ],
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "circular-deps": "skott --displayMode=file-tree",
        "dev": "vite",
        "preinstall": "npx only-allow pnpm",
        "lint": "pnpm run --parallel /^lint:/",
        "lint:eslint": "eslint src --quiet",
        "lint:prettier": "prettier --check 'src/**/*.{css,scss}'",
        "lint:styles": "stylelint 'src/**/*.{css,scss}'",
        "prepare": "husky",
        "preview": "vite preview",
        "test": "jest",
        "test:playwright": "playwright test",
        "typecheck": "tsc --noEmit"
    },
    "nano-staged": {
        "*.{css,scss}": [
            "stylelint --fix --quiet"
        ],
        "*.{ts,tsx}": [
            "eslint --fix --quiet"
        ],
        "*.{css,scss,md}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @gravity-ui/browserslist-config"
    ],
    "dependencies": {
        "@bem-react/classname": "^1.6.0",
        "@bmunozg/react-image-area": "^1.1.0",
        "@gravity-ui/components": "^4.0.1",
        "@gravity-ui/data-source": "^0.4.0",
        "@gravity-ui/icons": "^2.13.0",
        "@gravity-ui/navigation": "^3.3.3",
        "@gravity-ui/uikit": "^7.7.1",
        "@tanstack/react-form": "^0.41.4",
        "@tanstack/react-query": "^5.70.0",
        "@tanstack/react-query-devtools": "^5.70.0",
        "@tanstack/react-router": "^1.114.29",
        "@tanstack/zod-form-adapter": "^0.41.4",
        "bem-cn-lite": "^4.1.0",
        "jest-environment-jsdom": "^29.7.0",
        "ky": "^1.7.5",
        "lodash": "^4.17.21",
        "react": "^18.3.1",
        "react-compare-image": "^3.5.0",
        "react-diff-viewer": "^3.1.1",
        "react-dom": "^18.3.1",
        "react-image-diff": "^0.1.0",
        "react-syntax-highlighter": "^15.6.1",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.23.0",
        "@gravity-ui/browserslist-config": "^4.3.0",
        "@gravity-ui/illustrations": "^2.1.0",
        "@playwright/test": "^1.51.1",
        "@tanstack/eslint-plugin-router": "^1.114.29",
        "@tanstack/react-router-devtools": "^1.114.29",
        "@tanstack/router-plugin": "^1.114.29",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.16",
        "@types/node": "^22.13.14",
        "@types/react": "^18.3.20",
        "@types/react-dom": "^18.3.5",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@typescript-eslint/parser": "^8.28.0",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.10.0",
        "eslint-plugin-import-x": "^4.9.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "sass-embedded": "^1.86.0",
        "skott": "^0.35.4",
        "stylelint": "^16.17.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "ts-jest": "^29.3.0",
        "ts-node": "^10.9.2",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.28.0",
        "vite": "^6.2.3"
    },
    "engines": {
        "node": "22",
        "pnpm": "10"
    },
    "pnpm": {
        "ignoredBuiltDependencies": [
            "esbuild"
        ]
    }
}
